  "dependencies": {
    "@mazemasterjs/database-manager": "^1.3.0",
    "@mazemasterjs/logger": "^1.0.10",
    "@mazemasterjs/shared-library": "^1.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
